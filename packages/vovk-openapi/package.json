{
  "name": "vovk-openapi",
  "version": "0.0.1-draft.43",
  "description": "OpenAPI decorator and tools for Vovk.ts",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "npm-publish": "npm publish",
    "tsc": "tsc --noEmit",
    "ncu": "npm-check-updates -u"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
