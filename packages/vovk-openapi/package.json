  "keywords": [
    "vovk",
    "dto",
    "class-validator",
    "class-transformer"
  ],
  "author": "Andrey Gubanov",
  "license": "MIT",
  "peerDependencies": {
    "vovk": "^3.0.0-draft.97"
  },
  "dependencies": {
    "@stoplight/json-schema-sampler": "^0.3.0",
    "openapi3-ts": "^4.4.0"
  }
}
