{
  "name": "vovk-cli",
  "version": "0.0.1-beta.1",
  "bin": {
    "vovk": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "type": "module",
  "types": "./dist/index.d.mts",
  "prefix": ".",
  "scripts": {
    "build": "rm -rf dist tsconfig.build.tsbuildinfo && tsc -P tsconfig.build.json",
    "postbuild": "chmod +x ./dist/index.mjs",
    "pre-test": "npm run build",
    "test-only": "npm run pre-test && node --experimental-transform-types --experimental-strip-types --test --test-only test/spec/**/*.mts",
    "test": "npm run pre-test && node --experimental-transform-types --experimental-strip-types --test --test-concurrency=1 test/spec/**/*.mts",
    "tsc": "tsc --noEmit",
    "ncu": "npm-check-updates -u -x commander",
    "npm-publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finom/vovk.git"
  },
  "keywords": [
    "nextjs",
    "router",
    "cli",
    "vovk"
  ],
  "author": "Andrey Gubanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/finom/vovk/issues"
  },
  "homepage": "https://vovk.dev",
  "peerDependencies": {
    "vovk": "^3.0.0-beta.1",
    "vovk-ajv": "^0.0.0-beta.1",
    "vovk-client": "^0.0.4-beta.1",
    "vovk-python": "^0.0.1-beta.0",
    "vovk-rust": "^0.0.1-beta.0"
  },
  "optionalDependencies": {
    "@rolldown/binding-linux-x64-gnu": "1.0.0-beta.44"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "@inquirer/prompts": "^7.8.6",
    "@npmcli/package-json": "^7.0.1",
    "@types/json-schema": "^7.0.15",
    "chalk": "^5.6.2",
    "chokidar": "^4.0.3",
    "clone-deep": "^4.0.1",
    "commander": "^13.1.0",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "get-tsconfig": "^4.10.1",
    "glob": "^11.0.3",
    "gray-matter": "^4.0.3",
    "inflection": "^3.0.2",
    "jsonc-parser": "^3.3.1",
    "lodash": "^4.17.21",
    "loglevel": "^1.9.2",
    "openapi3-ts": "^4.5.0",
    "pluralize": "^8.0.0",
    "prettier": "^3.6.2",
    "tar-stream": "^3.1.7",
    "ts-morph": "^27.0.0",
    "type-fest": "^5.0.1",
    "undici": "^7.16.0",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@types/concat-stream": "^2.0.3",
    "@types/ejs": "^3.1.5",
    "@types/js-yaml": "^4.0.9",
    "@types/npmcli__package-json": "^4.0.4",
    "@types/pluralize": "^0.0.33",
    "@types/tar-stream": "^3.1.4",
    "concat-stream": "^2.0.0",
    "create-next-app": "^15.5.4",
    "http-server": "^14.1.1",
    "node-pty": "^1.0.0",
    "tsdown": "^0.16.0",
    "zod": "^4.1.11"
  }
}