{
  "name": "vovk-cli",
  "version": "0.0.1-draft.113",
  "bin": {
    "vovk": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "type": "module",
  "types": "./dist/index.d.mts",
  "prefix": ".",
  "scripts": {
    "build": "rm -rf dist tsconfig.build.tsbuildinfo && tsc -P tsconfig.build.json",
    "postbuild": "chmod +x ./dist/index.mjs",
    "pre-test": "npm run build",
    "test-only": "npm run pre-test && node --experimental-transform-types --experimental-strip-types --test --test-only test/spec/**/*.mts",
    "test": "npm run pre-test && node --experimental-transform-types --experimental-strip-types --test --test-concurrency=1 test/spec/**/*.mts",
    "tsc": "tsc --noEmit",
    "ncu": "npm-check-updates -u -x commander",
    "npm-publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finom/vovk.git"
  },
  "keywords": [
    "nextjs",
    "router",
    "cli",
    "vovk"
  ],
  "author": "Andrey Gubanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/finom/vovk/issues"
  },
  "homepage": "https://vovk.dev",
  "peerDependencies": {
    "vovk": "^3.0.0-draft.102"
  },
  "optionalDependencies": {
    "vovk-python-client": "*"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.3.1",
    "@npmcli/package-json": "^6.1.1",
    "chalk": "^5.4.1",
    "chokidar": "^4.0.3",
    "commander": "^13.1.0",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "gray-matter": "^4.0.3",
    "inflection": "^3.0.2",
    "jsonc-parser": "^3.3.1",
    "lodash": "^4.17.21",
    "loglevel": "^1.9.2",
    "pluralize": "^8.0.0",
    "prettier": "^3.4.2",
    "tar-stream": "^3.1.7",
    "ts-morph": "^25.0.0",
    "undici": "^7.3.0"
  },
  "devDependencies": {
    "@types/concat-stream": "^2.0.3",
    "@types/ejs": "^3.1.5",
    "@types/npmcli__package-json": "^4.0.4",
    "@types/pluralize": "^0.0.33",
    "@types/tar-stream": "^3.1.3",
    "concat-stream": "^2.0.0",
    "create-next-app": "^15.1.6",
    "node-pty": "^1.0.0",
    "type-fest": "^4.33.0"
  }
}