{
  "name": "vovk",
  "type": "module",
  "files": [
    "dist",
    "bin"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "version": "3.0.0-beta.140",
  "bin": {
    "vovk-cli-npx": "./bin/index.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./internal": {
      "types": "./dist/internal.d.ts",
      "default": "./dist/internal.js"
    },
    "./fetcher": {
      "types": "./dist/client/fetcher.d.ts",
      "default": "./dist/client/fetcher.js"
    },
    "./createRPC": {
      "types": "./dist/client/createRPC.d.ts",
      "default": "./dist/client/createRPC.js"
    },
    "./createValidateOnClient": {
      "types": "./dist/validation/createValidateOnClient.d.ts",
      "default": "./dist/validation/createValidateOnClient.js"
    }
  },
  "description": "Back-end Framework for Next.js App Router",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finom/vovk.git"
  },
  "scripts": {
    "postbuild": "chmod +x ./bin/index.mjs",
    "build": "rm -f *.tsbuildinfo && tsc",
    "rm-dist": "rm -rf mjs cjs",
    "lint": "eslint . --fix",
    "tsc": "tsc --noEmit",
    "npm-publish": "if [ -z \"$NPM_TAG\" ]; then echo 'Error: NPM_TAG is not set'; exit 1; fi; npm publish --tag=$NPM_TAG",
    "ncu": "npm-check-updates -u"
  },
  "keywords": [
    "nextjs",
    "router"
  ],
  "author": "Andrey Gubanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/finom/vovk/issues"
  },
  "homepage": "https://vovk.dev",
  "peerDependencies": {
    "openapi3-ts": "^4.5.0"
  }
}
