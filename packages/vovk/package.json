{
  "name": "vovk",
  "version": "3.0.0-beta.104",
  "main": "./cjs/index.js",
  "module": "./mjs/index.js",
  "types": "./mjs/index.d.ts",
  "bin": {
    "vovk-cli-npx": "./bin/index.mjs"
  },
  "exports": {
    ".": {
      "import": "./mjs/index.js",
      "require": "./cjs/index.js",
      "types": "./mjs/index.d.ts"
    },
    "./internal": {
      "import": "./mjs/internal.js",
      "require": "./cjs/internal.js",
      "types": "./mjs/internal.d.ts"
    },
    "./fetcher": {
      "import": "./mjs/client/fetcher.js",
      "require": "./cjs/client/fetcher.js",
      "types": "./mjs/client/fetcher.d.ts"
    },
    "./createRPC": {
      "import": "./mjs/client/createRPC.js",
      "require": "./cjs/client/createRPC.js",
      "types": "./mjs/client/createRPC.d.ts"
    }
  },
  "description": "Back-end Framework for Next.js App Router",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finom/vovk.git"
  },
  "scripts": {
    "postbuild": "chmod +x ./bin/index.mjs",
    "build": "rm -f *.tsbuildinfo && npm run build:cjs && npm run build:esm",
    "build:esm": "tsc --module NodeNext --outDir mjs --moduleResolution nodenext",
    "build:cjs": "tsc --module commonjs --outDir cjs",
    "rm-dist": "rm -rf mjs cjs",
    "lint": "eslint . --fix",
    "tsc": "tsc --noEmit",
    "npm-publish": "if [ -z \"$NPM_TAG\" ]; then echo 'Error: NPM_TAG is not set'; exit 1; fi; npm publish --tag=$NPM_TAG",
    "ncu": "npm-check-updates -u"
  },
  "keywords": [
    "nextjs",
    "router"
  ],
  "author": "Andrey Gubanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/finom/vovk/issues"
  },
  "homepage": "https://vovk.dev",
  "peerDependencies": {
    "next": "*",
    "openapi3-ts": "^4"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.1.0",
    "type-fest": "*"
  }
}
