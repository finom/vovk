  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.3",
    "@types/node": "^22.13.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10",
    "eslint-config-next": "15.1.6",
    "openapi3-ts": "^4.4.0",
