{
  "name": "test",
  "version": "0.1.1-beta.0",
  "private": true,
  "description": "Vovk test app",
  "license": "MIT",
  "author": "Andrey Gubanov",
  "scripts": {
    "ncu": "npm-check-updates -u && npm i",
    "dev": "npm run generate-modules-from-templates && npm run build-other-compiled-sources && node ../packages/vovk-cli/dist/index.mjs dev --next-dev -- --webpack",
    "dev-exit": "npm run generate-modules-from-templates && npm run build-other-compiled-sources && node ../packages/vovk-cli/dist/index.mjs dev --next-dev --exit -- --webpack",
    "build": "NODE_ENV=test next build",
    "start": "NODE_ENV=test next start",
    "lint": "next lint",
    "generate": "node ../packages/vovk-cli/dist/index.mjs generate",
    "bundle": "node ../packages/vovk-cli/dist/index.mjs bundle --keep-prebundle-dir",
    "generate:py": "node ../packages/vovk-cli/dist/index.mjs generate --composed-schema-json --template=python --composed-schema-json --out ./python",
    "generate-modules-from-templates": "sh ./scripts/generateModulesFromTemplates.sh",
    "build-other-compiled-sources": "rm -rf other-compiled-test-sources && npm run bundle && node ../packages/vovk-cli/dist/index.mjs generate --segmented-only"
  },
  "type": "module",
  "dependencies": {
    "@testing-library/react": "^16.3.0",
    "@valibot/to-json-schema": "^1.3.0",
    "arktype": "^2.1.26",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dto-mapped-types": "^0.0.7",
    "next": "^16.0.1",
    "react": "^19",
    "react-dom": "^19",
    "reflect-metadata": "^0.2.2",
    "tsdown": "^0.16.3",
    "valibot": "^1.1.0",
    "vovk-client": "^0.0.4-draft.127",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24.10.1",
    "@types/react": "19.2.3",
    "@types/react-dom": "^19",
    "autoprefixer": "^10",
    "eslint-config-next": "16.0.1",
    "openapi3-ts": "^4.5.0",
    "postcss": "^8",
    "tailwind": "^4.0.0",
    "tailwindcss": "^4"
  }
}
