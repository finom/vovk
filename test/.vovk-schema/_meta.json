{
  "$schema": "https://vovk.dev/api/schema/v3/meta.json",
  "package": {
    "name": "test",
    "version": "0.1.1-beta.0",
    "description": "Vovk test app",
    "author": "Andrey Gubanov",
    "license": "MIT",
    "main": "./index.cjs",
    "module": "./index.mjs",
    "types": "./index.d.mts",
    "exports": {
      ".": {
        "import": "./index.mjs",
        "require": "./index.cjs",
        "types": "./index.d.mts"
      },
      "./schema": {
        "import": "./schema.cjs",
        "require": "./schema.cjs",
        "types": "./schema.d.cts"
      },
      "./openapi": {
        "import": "./openapi.cjs",
        "require": "./openapi.cjs",
        "types": "./openapi.d.cts"
      }
    }
  },
  "config": {
    "libs": {},
    "generatorConfig": {
      "origin": "http://localhost:3210",
      "imports": {
        "validateOnClient": "../packages/vovk-ajv/index.js"
      },
      "openAPIObject": {
        "info": {
          "title": "Hello, OpenAPI!",
          "version": "1.0.0"
        },
        "servers": [
          {
            "url": "http://localhost:3000"
          }
        ]
      },
      "segments": {
        "client2": {
          "imports": {
            "fetcher": "./src/lib/fetcher.ts"
          }
        }
      }
    },
    "$schema": "https://vovk.dev/api/schema/v3/config.json"
  }
}